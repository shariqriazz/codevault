{
  "comment": "Recommended setup: OpenRouter with Nebius (embeddings) + Claude (chat) + Novita (reranking)",
  "defaultProvider": "openai",
  "providers": {
    "openai": {
      "apiKey": "your-openrouter-api-key-here",
      "baseUrl": "https://openrouter.ai/api/v1",
      "model": "qwen/qwen3-embedding-8b",
      "dimensions": 4096,
      "providerRouting": {
        "order": ["nebius", "deepinfra"],
        "allowFallbacks": true,
        "comment": "Primary: Nebius, Fallback: DeepInfra. Can also use siliconflow/fp8 for FP8 quantization"
      }
    }
  },
  "chatLLM": {
    "openai": {
      "apiKey": "your-openrouter-api-key-here",
      "baseUrl": "https://openrouter.ai/api/v1",
      "model": "anthropic/claude-sonnet-4.5",
      "maxTokens": 32000,
      "temperature": 0.1,
      "providerRouting": {
        "order": ["anthropic"],
        "allowFallbacks": true
      }
    }
  },
  "maxTokens": 32000,
  "rateLimit": {
    "rpm": 10000,
    "tpm": 600000
  },
  "reranker": {
    "apiUrl": "https://api.novita.ai/openai/v1/rerank",
    "apiKey": "your-novita-api-key-here",
    "model": "qwen/qwen3-reranker-8b"
  },
  "encryption": {
    "enabled": false,
    "key": ""
  }
}