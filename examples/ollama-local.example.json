{
  "comment": "Full local setup with Ollama - privacy-focused, no API costs",
  "defaultProvider": "openai",
  "providers": {
    "openai": {
      "baseUrl": "http://localhost:11434/v1",
      "model": "nomic-embed-text",
      "dimensions": 768
    }
  },
  "chatLLM": {
    "openai": {
      "baseUrl": "http://localhost:11434/v1",
      "model": "qwen2.5-coder:7b",
      "maxTokens": 32000,
      "temperature": 0.1
    }
  },
  "maxTokens": 8192,
  "rateLimit": {
    "rpm": null,
    "tpm": null
  },
  "encryption": {
    "enabled": false,
    "key": ""
  }
}
