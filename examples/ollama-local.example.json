{
  "comment": "CodeVault configuration for fully local setup with Ollama (no API costs)",
  "defaultProvider": "ollama",
  "providers": {
    "ollama": {
      "model": "nomic-embed-text",
      "dimensions": 768
    }
  },
  "chatLLM": {
    "ollama": {
      "model": "llama3.1"
    }
  },
  "maxTokens": 8192,
  "rateLimit": {
    "rpm": null,
    "tpm": null
  },
  "encryption": {
    "enabled": false
  }
}